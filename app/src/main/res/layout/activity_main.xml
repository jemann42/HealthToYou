<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.buttonfinal.MainActivity">


    <Button
        android:id="@+id/SOSButton"

        android:layout_width="105dp"
        android:layout_height="100dp"
        android:background="@drawable/buttonshapesos"

        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="SOS"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />


    <Button
        android:id="@+id/Heart"

        android:layout_width="101dp"
        android:layout_height="49dp"
        android:background="@drawable/buttonshape"

        android:onClick="clicked"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Heart"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.694" />

    <Button
        android:id="@+id/Trauma"

        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="14dp"

        android:layout_marginEnd="29dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="29dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/buttonshape"
        android:onClick="clicked"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Trauma"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/SOSButton"
        app:layout_constraintLeft_toRightOf="@+id/Allergies"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Allergies"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintVertical_bias="0.666"
        android:layout_marginTop="4dp" />

    <Button
        android:id="@+id/Cuts"

        android:layout_width="98dp"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"

        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        android:background="@drawable/buttonshape"
        android:onClick="clicked"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Cuts"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/SOSButton"
        tools:layout_constraintTop_creator="1" />


    <Button
        android:id="@+id/Poisoning"

        android:layout_width="155dp"
        android:layout_height="52dp"

        android:background="@drawable/buttonshape"
        android:onClick="clicked"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Poisoning"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintHorizontal_bias="0.873"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/Heart" />

    <Button
        android:id="@+id/Allergies"

        android:layout_width="152dp"
        android:layout_height="58dp"
        android:layout_marginBottom="14dp"

        android:layout_marginLeft="68dp"
        android:layout_marginStart="68dp"
        android:background="@drawable/buttonshape"
        android:onClick="clicked"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Allergies"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/SOSButton"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1" />

    <Button
        android:id="@+id/Fainting"

        android:layout_width="0dp"
        android:layout_height="51dp"
        android:layout_marginEnd="3dp"

        android:layout_marginLeft="11dp"
        android:layout_marginRight="3dp"
        android:layout_marginStart="11dp"
        android:background="@drawable/buttonshape"
        android:onClick="clicked"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Fainting"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/SOSButton"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="20dp"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/Others"
        android:layout_width="106dp"
        android:layout_height="48dp"
        android:text="Others"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="449dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.057" />


</android.support.constraint.ConstraintLayout>